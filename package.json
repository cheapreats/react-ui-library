{
    "name": "@cheapreats/react-ui",
    "version": "2.3.48",
    "description": "React UI library for CheaprEats",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "start": "start-storybook -p 6006",
        "deploy": "build-storybook --output-dir .deploy",
        "build": "rm -rf dist  && tsc && NODE_ENV=production babel dist -d dist",
        "test": "tsc && npm run eslint:fix",
        "prepublish": "npm run lint && npm run build",
        "eslint": "eslint ./src --ext tsx,ts,jsx,js",
        "eslint:fix": "npm run eslint -- --fix",
        "prettier": "prettier **/*.ts **/*.tsx **/*.js",
        "prettier:fix": "npm run prettier -- --write",
        "lint": "npm run prettier:fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CheaprEats/react-ui.git"
    },
    "author": "CheaprEats Inc.",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/CheaprEats/react-ui/issues"
    },
    "homepage": "https://github.com/CheaprEats/react-ui#readme",
    "peerDependencies": {
        "react": "*",
        "react-dom": "*",
        "styled-components": "*",
        "styled-icons": "*"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@storybook/addon-a11y": "^6.0.21",
        "@storybook/addon-actions": "^6.0.21",
        "@storybook/addon-knobs": "^6.0.21",
        "@storybook/addon-links": "^6.0.21",
        "@storybook/addon-toolbars": "^6.0.21",
        "@storybook/addon-viewport": "^6.0.21",
        "@storybook/addons": "^6.0.21",
        "@storybook/react": "^6.0.21",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.1.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "eslint": "^7.9.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.6",
        "husky": "^4.3.0",
        "prettier": "^2.1.2",
        "react": "^16.13.1",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^16.13.1",
        "styled-components": "^5.2.0",
        "styled-icons": "^10.21.0",
        "typescript": "^4.0.2"
    },
    "aliases": {
        "@Containers": "src/Containers",
        "@Layouts": "src/Fragments",
        "@Inputs": "src/Inputs",
        "@Text": "src/Text",
        "@Themes": "src/Themes",
        "@Utils": "src/Utils"
    },
    "eslintIgnore": [
        "stories",
        "scripts",
        "node_modules",
        "dist",
        ".storybook",
        ".deploy"
    ],
    "dependencies": {
        "@storybook/addon-backgrounds": "^6.0.21",
        "@storybook/addon-controls": "^6.0.21",
        "@storybook/addon-docs": "^6.0.21",
        "@types/emoji-mart": "^3.0.2",
        "@types/react-beautiful-dnd": "^13.0.0",
        "@types/react-dom": "^16.9.8",
        "@types/react-image-crop": "^8.1.2",
        "@types/recharts": "^1.8.15",
        "@types/storybook__react": "^5.2.1",
        "@types/styled-components": "^5.1.3",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "emoji-mart": "^3.0.0",
        "react-beautiful-dnd": "^13.0.0",
        "react-image-crop": "^8.6.5",
        "recharts": "^1.8.5"
    },
    "optionalDependencies": {
        "win-node-env": "^0.4.0"
    }
}
