{   
    "presets": ["@babel/preset-env", "@babel/preset-react","@babel/preset-typescript"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": "./src",
                "extensions": [".ts", ".tsx"],
                "alias": {
                    "@Containers": "./src/Containers",
                    "@Layouts": "./src/Fragments",
                    "@Inputs": "./src/Inputs",
                    "@Text": "./src/Text",
                    "@Themes": "./src/Themes",
                    "@Utils": "./src/Utils"
                }
            },
            "babel-plugin-styled-components"
        ],
        "recharts"
    ]
}
